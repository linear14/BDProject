<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolTip">
        <attr name="text" format="reference|string" />
        <attr name="direction" format="enum">
            <enum name="up" value="0" />
            <enum name="down" value="1" />
            <enum name="none" value="2" />
        </attr>
    </declare-styleable>
</resources>
